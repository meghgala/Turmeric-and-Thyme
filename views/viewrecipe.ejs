    <!--------------------------------------------------------------------------------------------->
    <section class="featured-recipes" id="blog">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="section-heading">
                        <span>Showing results for "rice"</span>
                    </div>
                </div> 
            </div> 
            <div class="row" id="recipeCardsContainer">
                <% recipesData.forEach(function(recipe) { %>
                    <div class="col-md-4 col-sm-6 col-xs-12">
                        <div class="featured-item">
                            <div class="thumb">
                                <img src="<%= recipe.imgSrc %>" alt="">
                                <div class="date-content">
                                    <h6><%= recipe.date %></h6>
                                </div>
                            </div>
                            <div class="down-content">
                                <h4><%= recipe.title %></h4>
                                <span><%= recipe.category %></span>
                                <p><%= recipe.description %></p>
                                <div class="row">
                                    <div class="col-md-6 first-button">
                                        <div class="text-button">
                                            <a href="#">Add to favorites</a>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="text-button">
                                            <a href="#">Continue Reading</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                <% }); %>
            </div>
        </div>
                <!---------->
            </div>
        </div>
    </section>
